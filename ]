#include<stdio.h>
#include<stdlib.h>
#include<getopt.h>
#include "converter.h"


int main(int argc, char** argv){
	
	int opt;
	int m_flag;
	int err_opt;

	err_opt = 0;
	m_flag = 0;
	while ((opt = getopt(argc, argv, "+c")) != -1){
		switch (opt){
			case 'c':
				m_flag = 1;
				break;
			default:
				err_opt =1;
				break;
		}
	}
	if (m_flag != 1){
		usage(argv[0]);
	}
	printf("%d\n",argc);
	if(argc < 1){
		printf("Test\n");
	}

	return 0;
}

void usage(char *name){
	fprintf(stderr, "usage: %s [-c] [arg ...]\n", name);
	exit(EXIT_FAILURE);
}
